{
  "name": "cypress2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    
    
    "test:cinema:chrome": "npx cypress run --record --group cinema --spec \"cypress\\e2e\\cinema.cy.js\" --browser chrome-stable",
    "test:pet:chrome": "npx cypress run --record --group pet --spec \"cypress\\e2e\\pet.cy.js\" --browser chrome-stable",
    "test:user:chrome": "npx cypress run --record --group user --spec \"cypress\\e2e\\user.cy.js\" --browser chrome-stable",
    "test:parallel:chrome": "run-p -c test:pet:chrome test:cinema:chrome test:user:chrome",
        
    "test:cinema:msedge": "npx cypress run --record --group cinema --spec \"cypress\\e2e\\cinema.cy.js\" --browser edge",
    "test:pet:msedge": "npx cypress run --record --group pet --spec \"cypress\\e2e\\pet.cy.js\" --browser edge",
    "test:user:msedge": "npx cypress run --record --group user --spec \"cypress\\e2e\\user.cy.js\" --browser edge",
    "test:parallel:msedge": "run-p -c test:pet:msedge test:cinema:msedge test:user:msedge",
  
    "test:first:electron": "npx cypress run --record --group first --spec \"cypress\\e2e\\first\" --browser electron",
    "test:pet:electron": "npx cypress run --record --group pet --spec \"cypress\\e2e\\pet.cy.js\" --browser electron",
    "test:second:electron": "npx cypress run --record --group ace --spec \"cypress\\e2e\\second\" --browser electron",
    "test:parallel:electron": "run-p -c test:pet:electron test:first:electron test:second:electron"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "all": "^0.0.0",
    "cypress": "^13.13.0",
    "npm": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "run": "^1.5.0"
  }
}
